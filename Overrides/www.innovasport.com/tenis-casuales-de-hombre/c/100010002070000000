	var ecommerceData = {
	  'ecommerce': {
		    'currencyCode': 'MXN',                      
		    	'impressions': impressions
			  }
		};

	dataLayer.push(ecommerceData)

	function gtmAddToCart(productCode, quantityAdded) {
		dataLayer.push({
			'event' : 'addToCart',
			'ecommerce' : {
				'currencyCode' : 'MXN',
				'add' : {
					'products' : [ {
						'name' : "", 
						'id' : productCode,
						'price' : '',
						'brand' : "",
						'category' : category,
						'variant' : variants,
						'quantity' : quantityAdded
					} ]
				}
			}
		});
	}

	function gtmUpdateAddToCart(productCode, quantityAdded) {
		dataLayer.push({
			'event' : 'addToCart',
			'ecommerce' : {
				'currencyCode' : 'MXN',
				'add' : {
					'products' : [ {
						'id' : productCode,
						'quantity' : quantityAdded
					} ]
				}
			}
		});
	}

	function gtmProductClick(productData) {
		dataLayer.push({
			'event' : 'productClick',
			'ecommerce' : {
				'click' : {
					'actionField' : {
						'list' : 'product click list'
					},
					'products' : [ {
						'name' : productData.productName,
						'id' : productData.productCode,
						'price' : '',
						'brand' : '',
						'category' : '',
						'variant' : '',
						'position' : ''
					} ]
				}
			}
		});
	}

	function gtmTrackPromotionClick(promotionData) {
		dataLayer.push({
			'event' : 'promotionClick',
			'ecommerce' : {
				'promoClick' : {
					'promotions' : [ {
						'id' : promotionData.promoCode,
						'name' : promotionData.promoName,
						'creative' : '',
						'position' : ''
					} ]
				}
			}
		});
	}

	function gtmRemoveFromCart(productCode, initialQuantity) {
		dataLayer.push({
			'event' : 'removeFromCart',
			'ecommerce' : {
				'remove' : {
					'products' : [ {
						'name' : '',
						'id' : productCode,
						'price' : '',
						'brand' : '',
						'category' : '',
						'variant' : '',
						'quantity' : initialQuantity
					} ]
				}
			}
		});
	}

	function gtmUpdateCart(productCode, initialCartQuantity, newCartQuantity) {
		if (initialCartQuantity != newCartQuantity) {
			if (initialCartQuantity > newCartQuantity) {
				gtmRemoveFromCart(productCode, initialCartQuantity
						- newCartQuantity);
			} else {
				gtmUpdateAddToCart(productCode, newCartQuantity - initialCartQuantity);
			}
		}
	}

	function gtmMenuClick(nameMenu){
		dataLayer.push({
			'event' : 'Menu Click',
			'nameMenu': nameMenu
		});
	}

	window.mediator.subscribe('trackAddToCart', function(data) {
		if (data.productCode && data.quantity) {
			gtmAddToCart(data.productCode, data.quantity);
		}
	});

	window.mediator.subscribe('trackRemoveFromCart', function(data) {
		if (data.productCode && data.initialCartQuantity) {
			gtmRemoveFromCart(data.productCode, data.initialCartQuantity);
		}
	});

	window.mediator.subscribe('trackUpdateCart', function(data) {
		if (data.productCode && data.initialCartQuantity
				&& data.newCartQuantity) {
			gtmUpdateCart(data.productCode, data.initialCartQuantity,
					data.newCartQuantity);
		}
	});

	window.mediator.subscribe('gtmTrackProductClick', function(data) {
		if (data.productData) {
			gtmProductClick(data.productData);
		}
	});

	window.mediator.subscribe('gtmTrackPromotionClick', function(data) {
		if (data.promotionData) {
			gtmTrackPromotionClick(data.promotionData);
		}
	});

	window.mediator.subscribe('gtmMenuClick',function(data){
		gtmMenuClick(data.menuPathName);
	})
</script>

