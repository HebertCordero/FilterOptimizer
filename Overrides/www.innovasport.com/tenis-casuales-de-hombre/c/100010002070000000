      $(document).ready(function () {

        $('.button-footer').click(function () {
          $('.hidden-prefooter').toggleClass('hidden');

          if ($(".showButton").text() == ("VER MÁS")) {
            $(".showButton").text("VER MENOS")
          }
          else { $(".showButton").text("VER MÁS") }


        });
      });

